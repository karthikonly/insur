<h1>Listing Quotes</h1>

<table>
  <thead>
    <tr>
      <th>Agent code</th>
      <th>Effective date</th>
      <th>Line</th>
      <th>State</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotes.each do |quote| %>
      <tr>
        <td><%= quote.agent_code %></td>
        <td><%= quote.effective_date %></td>
        <td><%= quote.line %></td>
        <td><%= quote.state %></td>
        <td><%= quote.user.try(:name) %></td>
        <td><%= link_to 'Show', quote %></td>
        <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
        <td><%= link_to 'Destroy', quote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote', new_quote_path %>
